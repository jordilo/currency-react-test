    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-image-snapshot": "^6.4.0",
    "prettier": "3.3.3",
    "react-router-dom": "^6.26.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2"
  },
  "overrides": {
    "typescript": "^5.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "unused-imports"
    ],
    "rules": {
      "unused-imports/no-unused-imports": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "printWidth": 90,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "singleQuote": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
